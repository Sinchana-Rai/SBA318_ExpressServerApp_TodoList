<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
</head>
<body>
    <h1>Users</h1>

    <% if (users.length > 0) { %>
        <ul>
            <% users.forEach((user) => { %>
                <li>
                    <strong>Name:</strong> <%= user.name %><br>
                    <strong>Username:</strong> <%= user.username %><br>
                    <strong>Email:</strong> <%= user.email %>
                </li>
            <% }); %>
        </ul>
    <% } else { %>
        <p>No users found.</p>
    <% } %>

    <a href="/todo">Back to Todo List</a>
</body>
</html>
